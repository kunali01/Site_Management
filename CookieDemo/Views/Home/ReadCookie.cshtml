@{
}
@{
    ViewData["Title"] = "Read Cookie and Session";
}

<h1>Read Cookie and Session</h1>

@if (ViewBag.CookieEmail != null)
{
    <h4>Cookie Email: @ViewBag.CookieEmail</h4>
}
else
{
    <h4>No Cookie Found</h4>
}

@if (ViewBag.SessionEmail != null)
{
    <h4>Session Email: @ViewBag.SessionEmail</h4>
}
else
{
    <h4>No Session Found</h4>
}
